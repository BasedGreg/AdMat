<?xml version="1.0" encoding="utf-8" ?>

<Defs>

  <RecipeDef>
    <defName>Make_GrapheneCartridge</defName>
    <label>make chemical vapor cartridge</label>
    <description>Make a chemical vapor cartridge.</description>
    <jobString>Making chemical vapor cartridge.</jobString>
    <workSpeedStat>SmithingSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <workAmount>3000</workAmount>
    <unfinishedThingDef>UnfinishedComponent</unfinishedThingDef>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Chemfuel</li>
          </thingDefs>
        </filter>
        <count>10</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Graphite</li>
          </thingDefs>
        </filter>
        <count>10</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Chemfuel</li>
        <li>Graphite</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <GrapheneCartridge>1</GrapheneCartridge>
    </products>
    <researchPrerequisite>AdvancedMaterials</researchPrerequisite>
    <skillRequirements>
      <Crafting>8</Crafting>
    </skillRequirements>
    <workSkill>Crafting</workSkill>
  </RecipeDef>
  
    <RecipeDef>
    <defName>Make_PlasteneFilament</defName>
    <label>make nano-printer filament</label>
    <description>Make a nano-printer filament.</description>
    <jobString>Making nano-printer filament.</jobString>
    <workSpeedStat>SmithingSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <workAmount>1000</workAmount>
    <unfinishedThingDef>UnfinishedComponent</unfinishedThingDef>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Plasteel</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Graphene</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Plasteel</li>
        <li>Graphene</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <PlasteneFilament>1</PlasteneFilament>
    </products>
    <researchPrerequisite>AdvancedMaterials</researchPrerequisite>
    <skillRequirements>
      <Crafting>8</Crafting>
    </skillRequirements>
    <workSkill>Crafting</workSkill>
  </RecipeDef>

  </Defs>